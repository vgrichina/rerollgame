{
  "private": true,
  "name": "rerollgame",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "deploy": "npm run build && devvit upload",
    "dev": "devvit playtest",
    "launch": "npm run deploy && devvit publish",
    "login": "devvit login",
    "build:testbed": "vite build --config vite.config.testbed.js",
    "testbed": "npm run build:testbed && node --import ./src/testbed/loader.js src/testbed/server.js",
    "testbed:watch": "node --import ./src/testbed/loader.js --watch src/testbed/server.js"
  },
  "engines": {
    "node": ">=22.2.0"
  },
  "dependencies": {
    "@devvit/start": "0.12.12",
    "@devvit/web": "0.12.12",
    "@hono/node-server": "1.19.8",
    "devvit": "0.12.12",
    "hono": "4.11.4",
    "quickjs-emscripten": "^0.31.0",
    "quickjs-emscripten-core": "^0.31.0",
    "@jitl/quickjs-singlefile-cjs-release-sync": "^0.31.0"
  },
  "devDependencies": {
    "dotenv": "^16.4.0",
    "redis": "^4.7.0",
    "vite": "7.3.1",
    "vite-plugin-wasm": "^3.5.0"
  }
}
